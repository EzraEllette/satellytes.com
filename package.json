    "eslint-plugin-react": "7.20.0",
    "husky": "4.2.5",
    "jest": "^26.0.1",
    "jsdom": "^16.2.2",
    "lint-staged": "10.2.6",
    "prettier": "2.0.5",
    "ts-jest": "^26.0.0",
    "typescript": "3.9.3"
